// Author: Caleb Pollock
// A collection of tests to determine whether
// the datapath interpreter is functioning
// correctly, covers RAW, WAR, and WAW hazards,
// as well as forwarding in the ALU
LOAD   0xAF  
ADD    0xAE // ALU operations in sequence
ADD    0xAD
ADD    0xAC
ADD    0xAB
ADD    0xAA
STORE  0xA9
AND    0xA8 // clear things out
AND    0xA8
AND    0xA8
AND    0xA8
JZ     0x11 // branch hazard
OR     0xA8
OR     0xA8
OR     0xA8
OR     0xA8
JMP    0x2b 
LOAD   0xA7 // series of load and stores
ADD    0xAF // to the same address
STORE  0xA7
LOAD   0xA7 
ADD    0xAF
STORE  0xA7
ADD    0xAF
STORE  0xA7
STORE  0xA7
LOAD   0xA7
LOADI  0xA6
STOREI 0xA6
LOAD   0xA7
STOREI 0xA6
LOADI  0xA6
STORE  0xA7
ADD    0xA7
STORE  0xA7
ADD    0xA7
STOREI 0xA6
STORE  0x25 // write to ourselves
STORE  0x25 // write to the guy behind us
LOAD   0x27 // load our own address
STORE  0x29 // write to the next address
LOAD   0x27 // prior read dependency
JMP    0x0C 
HALT
0xA6: 167
0xA7: 1
0xA8: 0
0xA9: 0
0xAA: 6
0xAB: 5
0xAC: 4
0xAD: 3
0xAE: 2
0xAF: 1
